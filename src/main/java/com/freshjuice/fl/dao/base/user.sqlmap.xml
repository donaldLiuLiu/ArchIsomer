<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.freshjuice.fl.dao.base.IUserMapper">
	

	<select id="getUserById" parameterType="java.lang.String" resultType="user">
		select userId,userName,userPswd,phone from k_user where userId = #{value, jdbcType=VARCHAR}
	</select>


</mapper>